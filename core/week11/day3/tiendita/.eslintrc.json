{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "eslint-config-prettier",
    "prettier",
    "standard"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-unused-vars": ["warn"],
    "space-before-function-paren": ["off"],
    "comma-dangle": ["off"],
    "react/prop-types": ["off"],
    "multiline-ternary": ["off"]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
